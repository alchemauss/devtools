{
  "name": "@mauss/devtools",
  "author": "Ignatius Bagus",
  "description": "Mauss development toolkit",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "svelte": "svelte/index.js",
  "scripts": {
    "clean": "git clean -dfx -e node_modules",
    "preversion": "tsc --noEmit",
    "version": "node update.mjs && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm ci && tsc -d && prettier -wu utils/**"
  },
  "prettier": "mauss/prettier.json",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./svelte": {
      "import": "./svelte/index.js"
    },
    "./utils": {
      "import": "./utils/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devmauss/devtools.git"
  },
  "bugs": {
    "url": "https://github.com/devmauss/devtools/issues"
  },
  "keywords": [
    "development",
    "toolkit"
  ],
  "peerDependencies": {
    "svelte": "^3.49.0",
    "syv": "^0.0.13"
  },
  "devDependencies": {
    "mauss": "^0.3.3",
    "prettier": "^2.2.1",
    "syv": "^0.0.13",
    "typescript": "^4.2.4"
  }
}
